      }]
    },
    {
      "name": "(G) jason bourne",
      "weight": 3,
      "events": [{
        "type": "SearchAndClick",
        "arguments": {
          "queryText": "jason bourne",
          "matchField": "title",
          "matchPattern": " Identity$",
          "probability": 1
        }
      }]
    },
    {
      "name": "(G) balboa",
      "weight": 3,
      "events": [{
        "type": "SearchAndClick",
        "arguments": {
          "queryText": "balboa",
